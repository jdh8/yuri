  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owm111/knife-wife/issues"
  },
  "homepage": "https://github.com/owm111/knife-wife#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "common-tags": "^1.8.0",
    "discord.js": "^11.3.2"
  },
  "devDependencies": {
    "@citycide/cz-conventional": "^1.0.0",
    "commitizen": "^2.9.6",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.17.2"
  },
